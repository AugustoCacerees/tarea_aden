<?xml version="1.0" encoding="utf-8"?>
<odoo>
    #Vista de Arbol
    <record id="view_inscripcion_tree" model="ir.ui.view">
            <field name="name">modulo_alumnos.inscripcion.tree</field>
            <field name="model">modulo_alumnos.inscripcion</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="alumnos_id" string="Alumno"/>
                    <field name="programa_id" string="Programa"/>
                </tree>
            </field>
    </record>

    #Vista de Formulario
    <record id="view_inscripcion_form" model="ir.ui.view">
            <field name="name">modulo_alumnos.inscripcion.form</field>
            <field name="model">modulo_alumnos.inscripcion</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="alumnos_id"/>
                            </group>
                            <group>
                                <field name="programa_id"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
    </record>
</odoo>